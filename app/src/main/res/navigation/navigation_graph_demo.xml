<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph_demo"
    app:startDestination="@id/navHomeFragment">

    <!--click `` to add a destination
        -> Create new destination
    -->

    <fragment
        android:id="@+id/navHomeFragment"
        android:name="com.adnroidlearningkts.navigation.fragments.NavHomeFragment"
        android:label="fragment_nav_home"
        tools:layout="@layout/fragment_nav_home" >
        <action
            android:id="@+id/action_navHomeFragment_to_navOneFragment"
            app:destination="@id/navOneFragment" />
    </fragment>
    <fragment
        android:id="@+id/navOneFragment"
        android:name="com.adnroidlearningkts.navigation.fragments.NavOneFragment"
        android:label="fragment_nav_one"
        tools:layout="@layout/fragment_nav_one" />
</navigation>